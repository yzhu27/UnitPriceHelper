	            getAjaxMessageJson('/MessagesJS?langId=-1&storeId=10301&isPharmacy=false&getJsonValue=true');
	            function getAjaxMessageJson(messageJsonURL){
                   $.ajax({
                        type: 'get',
                        url: messageJsonURL,
                        dataType: 'json',
                        success: function(data) {
                            if(data != null){
                                messages = data;
                            }                        
                        },
                        error: function(xHR, status, error) {
                            console.log("Failed to retrieve the Message Json",error);
                        }
                    })
               }
